{
  "bookmarks.gutterIconBorderColor": "#83a598",
  "bookmarks.gutterIconFillColor": "#83a598",
  "breadcrumbs.enabled": false,
  "debug.toolBarLocation": "docked",
  "editor.codeLens": false,
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": "'calt', 'ss01'",
  "editor.fontSize": 16,
  "editor.guides.indentation": false,
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.renderWhitespace": "trailing",
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "elixirLS.dialyzerEnabled": false,
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": { "*.html.eex": "html" },
  "files.hotExit": "onExitAndWindowClose",
  "git.openRepositoryInParentFolders": "always",
  "gruvboxMaterial.darkContrast": "soft",
  "gruvboxMaterial.darkPalette": "material",
  "gruvboxMaterial.darkWorkbench": "material",
  "gruvboxMaterial.italicComments": true,
  "gruvboxMaterial.italicKeywords": false,
  "gruvboxMaterial.lightContrast": "soft",
  "gruvboxMaterial.lightPalette": "material",
  "gruvboxMaterial.lightWorkbench": "material",
  "html.format.wrapLineLength": 0,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.folders.color": "#a69883",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 0.5,
  "npm.packageManager": "npm",
  "python.languageServer": "Pylance",
  "python.linting.pylintEnabled": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.enabled": false,
  "search.smartCase": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "kitty",
  "terminal.external.osxExec": "kitty",
  "terminal.external.windowsExec": "wt.exe",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "pwsh",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.mouseWheelScrollSensitivity": 2,
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "icon": "terminal",
      "path": "zsh"
    }
  },
  "terminal.integrated.profiles.windows": {
    "pwsh": {
      "args": ["-NoLogo"],
      "icon": "terminal-powershell",
      "path": "pwsh.exe"
    }
  },
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  "window.commandCenter": true,
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "[Gruvbox Material Dark]": {
      "bookmarks.overviewRuler": "#83a598",
      "editorStickyScrollHover.background": "#32302f",
      "terminalCursor.foreground": "#928374"
    },
    "[Gruvbox Material Light]": {
      "bookmarks.overviewRuler": "#83a598",
      "editorStickyScrollHover.background": "#f2e5bc",
      "terminalCursor.foreground": "#928374"
    }
  },
  "workbench.colorTheme": "Gruvbox Material Light",
  "workbench.editor.showTabs": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "none",
  "yaml.validate": false,
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ]
}

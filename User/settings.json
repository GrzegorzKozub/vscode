{
  "breadcrumbs.enabled": false,
  "chat.agent.enabled": true,
  "chat.editor.fontSize": 14,
  "cSpell.useCustomDecorations": false,
  "debug.allowBreakpointsEverywhere": true,
  "debug.toolBarLocation": "docked",
  "diffEditor.experimental.showMoves": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.codeLens": false,
  "editor.codeLensFontFamily": "Cascadia Code",
  "editor.codeLensFontSize": 14,
  "editor.colorDecoratorsLimit": 1024,
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 4,
  "editor.experimentalEditContextEnabled": true,
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": "'calt', 'ss01'",
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": false,
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80],
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tokenColorCustomizations": { "[*]": { "textMateRules": [] } },
  "editor.wordWrapColumn": 80,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.hotExit": "onExitAndWindowClose",
  "git.blame.editorDecoration.enabled": false,
  "git.blame.editorDecoration.template": "${subject}, ${authorName}, ${authorDateAgo}, ${hashShort}",
  "git.blame.statusBarItem.enabled": true,
  "git.blame.statusBarItem.template": "${authorName}, ${authorDateAgo}, ${hashShort}",
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.enable": {
    "*": false,
    "csharp": true,
    "cucumber": true,
    "dockercompose": true,
    "dockerfile": true,
    "go.mod": true,
    "go": true,
    "html": true,
    "javascript": true,
    "json": true,
    "jsonc": true,
    "lua": true,
    "python": true,
    "rust": true,
    "sql": true,
    "toml": true,
    "typescript": true,
    "xml": true,
    "yaml": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.gitProtocol": "ssh",
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.showWelcome": false,
  "go.survey.prompt": false,
  "gopls": { "ui.diagnostic.analyses": { "shadow": true } },
  "html.format.wrapLineLength": 0,
  "isort.args": ["--profile", "black"],
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "Lua.hint.enable": true,
  "Lua.hint.setType": true,
  "markdown.preview.fontSize": 16,
  "markdown.preview.markEditorSelection": false,
  "markdown.validate.enabled": true,
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.folders.color": "#928374", // grey1
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.saturation": 0.5,
  "npm.packageManager": "npm",
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.experiments.optInto": ["pythonTestAdapter"],
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.inlayHints.closureCaptureHints.enable": true,
  "rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
  "rust-analyzer.inlayHints.discriminantHints.enable": "always",
  "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
  "rust-analyzer.inlayHints.implicitDrops.enable": true,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
  "rust-analyzer.inlayHints.rangeExclusiveHints.enable": true,
  "rust-analyzer.inlayHints.reborrowHints.enable": "always",
  "security.promptForLocalFileProtocolHandling": false,
  "security.workspace.trust.enabled": false,
  "search.smartCase": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.linuxExec": "kitty",
  "terminal.external.windowsExec": "wezterm-gui.exe",
  "terminal.integrated.bellDuration": 3000,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "pwsh",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.fontFamily": "Cascadia Code NF",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontLigatures.featureSettings": "\"calt\" on, \"ss01\" on",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.mouseWheelScrollSensitivity": 0.5,
  "terminal.integrated.profiles.linux": {
    "zsh": { "icon": "terminal", "path": "zsh" }
  },
  "terminal.integrated.profiles.windows": {
    "pwsh": {
      "args": ["-NoLogo"],
      "icon": "terminal-powershell",
      "path": "pwsh.exe"
    }
  },
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.title": "${process}${separator}${progress}",
  "terminal.integrated.tabStopWidth": 2,
  "terminal.integrated.windowsUseConptyDll": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": false,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": false,
  "window.commandCenter": true,
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": { "[*]": {} },
  "workbench.colorTheme": "Gruvbox Material Flat Dark",
  "workbench.editor.empty.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "Gruvbox Material Flat Dark",
  "workbench.preferredLightColorTheme": "Gruvbox Material Flat Light",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.renderIndentGuides": "none",
  "yaml.format.singleQuote": true,
  "yaml.validate": false,
  "[cucumber]": { "editor.semanticHighlighting.enabled": true },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[python]": { "editor.defaultFormatter": "ms-python.black-formatter" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" }
}

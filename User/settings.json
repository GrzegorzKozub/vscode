{
  "breadcrumbs.enabled": false,
  "chat.editor.fontSize": 14,
  "cSpell.useCustomDecorations": false,
  "debug.allowBreakpointsEverywhere": true,
  "debug.toolBarLocation": "docked",
  "editor.bracketPairColorization.enabled": false,
  "editor.codeLens": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 4,
  "editor.experimentalEditContextEnabled": true,
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": "'calt', 'ss01'",
  "editor.fontSize": 16,
  "editor.guides.indentation": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.renderWhitespace": "none",
  "editor.rulers": [80],
  "editor.semanticHighlighting.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tokenColorCustomizations": {
    "[Gruvbox Material *]": {
      "textMateRules": [
        {
          "name": "punctuation",
          "scope": [
            "fenced_code.block.language",
            "meta.brace",
            "meta.tag.preprocessor.xml",
            "meta.tag.xml",
            "punctuation.definition.block.sequence.item.yaml",
            "punctuation.definition.interpolation.begin.cs",
            "punctuation.definition.interpolation.end.cs",
            "punctuation.definition.list.begin.markdown",
            "punctuation.definition.markdown",
            "punctuation.definition.raw.markdown",
            "punctuation.definition.string.begin.html",
            "punctuation.definition.string.begin.xml",
            "punctuation.definition.string.begin.yaml",
            "punctuation.definition.string.end.html",
            "punctuation.definition.string.end.xml",
            "punctuation.definition.string.end.yaml",
            "punctuation.definition.table.toml",
            "punctuation.definition.tag.begin.html",
            "punctuation.definition.tag.end.html",
            "punctuation.definition.tag.xml",
            "punctuation.definition.template-expression",
            "punctuation.separator.key-value.html",
            "punctuation"
          ],
          "settings": { "foreground": "#928374" }
        }
      ]
    },
    "[Gruvbox Material Dark]": {
      "textMateRules": [
        {
          "name": "comments",
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": { "fontStyle": "italic", "foreground": "#7c6f64" }
        },
        {
          "name": "json & yaml names",
          "scope": ["entity.name.tag.yaml", "support.type.property-name.json"],
          "settings": { "foreground": "#7daea3" }
        },
        {
          "name": "markdown link text",
          "scope": ["string.other.link.title.markdown"],
          "settings": { "foreground": "#d3869b" }
        },
        {
          "name": "markdown link url",
          "scope": ["markup.underline.link.markdown"],
          "settings": { "fontStyle": "", "foreground": "#7daea3" }
        }
      ]
    },
    "[Gruvbox Material Light]": {
      "textMateRules": [
        {
          "name": "comments",
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": { "fontStyle": "italic", "foreground": "#a89984" }
        },
        {
          "name": "json & yaml names",
          "scope": ["entity.name.tag.yaml", "support.type.property-name.json"],
          "settings": { "foreground": "#45707a" }
        },
        {
          "name": "markdown link text",
          "scope": ["string.other.link.title.markdown"],
          "settings": { "foreground": "#945e80" }
        },
        {
          "name": "markdown link url",
          "scope": ["markup.underline.link.markdown"],
          "settings": { "fontStyle": "", "foreground": "#45707a" }
        }
      ]
    }
  },
  "editor.wordWrapColumn": 80,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.hotExit": "onExitAndWindowClose",
  "git.openRepositoryInParentFolders": "always",
